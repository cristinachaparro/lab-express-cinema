<div id="all-movies-container">
  <h1>Ironhack Cinema</h1>
  <h4>Click on the movie to check the showtimes!</h4>
  <div id="all-movies">
    {{#each this.allMovies}}
    <div class="each-movie">
      <img src="{{this.image}}" alt="movie cover">
      <h4>{{this.title}}</h4>
      <a href="/movie/{{this.id}}">See more</a>
    </div>
    {{/each}}
  </div>
</div>